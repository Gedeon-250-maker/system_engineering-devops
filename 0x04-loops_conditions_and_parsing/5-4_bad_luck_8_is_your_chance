#!/usr/bin/env bash
# This script displays different messages based on loop iteration using if-elif-else.

COUNT=1
while [ "$COUNT" -le 10 ]; do
  if [ "$COUNT" -eq 4 ]; then
    echo "bad luck"
  elif [ "$COUNT" -eq 8 ]; then
    echo "good luck"
  else
    echo "Best School"
  fi
  ((COUNT++))
done
